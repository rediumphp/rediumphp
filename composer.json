{
	"name": "likore/rediumphp",
	"description": "A lightweight, modern PHP framework for rapid API development",
	"type": "library",
	"version": "0.1.0-dev",
	"license": "MIT",
	"authors": [
		{
			"name": "Likore",
			"email": "likoremba@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Redium\\": "src/"
		},
		"files": [
			"src/Utils/Helpers.php"
		]
	},
	"require": {
		"php": "^8.1",
		"slim/slim": "^4.14",
		"slim/psr7": "^1.7",
		"lcobucci/jwt": "^5.5",
		"vlucas/phpdotenv": "^5.6",
		"ramsey/uuid": "^4.7",
		"ext-pdo": "*",
		"ext-json": "*",
		"symfony/serializer": "^6.4",
		"symfony/property-access": "^6.4",
		"symfony/property-info": "^6.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan": "^1.10",
		"symfony/var-dumper": "^7.4"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
